#include <set>
vector < int > sortedArray(vector < int > a, vector < int > b) {
    set<int> ans;
    vector<int> anss;
    for(int i=0;i<a.size();i++){
        ans.insert(a[i]);
        
    }
     for(int j=0;j<b.size();j++){
        ans.insert(b[j]);
        
    }
   // sort(ans.begin(),ans.end());
   for(auto it:ans){
anss.push_back(it);
   }
    return anss;
}
